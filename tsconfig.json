{
  "compilerOptions": {
    "outDir": "lib",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es5", "es2015", "es2017", "dom"],
    "sourceMap": true,
    "moduleResolution": "node",
    "rootDirs": ["src"],
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true,
    "declaration": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
